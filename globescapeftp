<decoder name="Globescape EFT">
   <parent>windows-date-format</parent>
   <prematch offset="after_parent">\d*.\d*.\d*.\d* -|\d* \S*\\\S*</prematch>
   <regex offset="after_parent">(\d+.\d+.\d+.\d+) - (\S+\\\S+) </regex>
   <order>IP, Data</order>
</decoder>

<decoder name="Globescape EFT">
    <parent>windows-date-format</parent>
    <regex offset="after_parent">[\d+](\S+) /(\.+) - (\.+)</regex>
    <order>action, file, extra-data</order>
</decoder>